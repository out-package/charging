    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-prettier": "3.4.0",
    "husky": "6.0.0",
    "lint-staged": "10.5.4",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.13",
    "prettier": "2.2.1",
    "react-app-rewired": "^2.1.8",
    "react-snap": "^1.23.0",
    "sass-loader": "^11.0.1",
    "tailwindcss": "^2.1.2",
    "typescript": "^4.1.2",
    "vite": "^2.2.3"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint --fix"
